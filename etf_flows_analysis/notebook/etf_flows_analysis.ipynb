import pandas as pd
import matplotlib.pyplot as plt
import seaborn as sns

df = pd.read_csv("ETFbook-data-analytics-case-study/etf_flows_analysis/data/etf_flows_2025_export.csv")
df.head()

df["flow_rank"] = (
    df.groupby("date")["net_flow"]
      .rank(method="dense", ascending=False)
)

df.sort_values(["date", "flow_rank"]).head(10)

top_3 = df[df["flow_rank"] <= 3]
top_3

plt.figure(figsize=(12,6))
sns.lineplot(
    data=df,
    x="date",
    y="net_flow",
    hue="etf_ticker"
)
plt.title("ETF Net Flows Over Time")
plt.show()
